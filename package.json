{
  "name": "node_boilerplate_typescript",
  "version": "1.0.0",
  "private": true,
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "npm run build && node build/index.js",
    "start:dev": "npx nodemon ./src/index.ts",
    "build": "rimraf ./build && tsc",
    "typeorm": "node ./node_modules/typeorm/cli.js",
    "typeorm:dev": "npx cross-env NODE_ENV=development ts-node ./node_modules/typeorm/cli.js",
    "typeorm:test": "npx cross-env NODE_ENV=test ts-node ./node_modules/typeorm/cli.js",
    "db:update": "npm run build && npm run typeorm migration:run",
    "db:update:dev": "npm run typeorm:dev migration:run",
    "db:update:test": "npm run typeorm:test migration:run",
    "db:generate:dev": "npm run typeorm:dev migration:generate -- -n",
    "test": "npx cross-env NODE_ENV=test mocha --exit --timeout 15000 -r ts-node/register --file ./test/setup.ts './test/**/*.test.ts'",
    "test:watch": "npx cross-env NODE_ENV=test mocha --timeout 15000 -r ts-node/register -w -R min --file ./test/setup.ts './test/**/*.test.ts'",
    "lint": "eslint . --ext .ts",
    "lint-fix": "eslint . --ext .ts --fix"
  },
  "keywords": [],
  "author": "Sandeep Singh",
  "engines": {
    "node": "16.x"
  },
  "license": "ISC",
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/express": "github:types/express",
    "@types/express-jwt": "^6.0.4",
    "@types/node": "^16.11.7",
    "@types/supertest": "^2.0.11",
    "@types/swagger-ui-express": "^4.1.3",    
    "@typescript-eslint/eslint-plugin": "^5.3.1",
    "@typescript-eslint/parser": "^5.3.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.2.0",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-mocha": "^8.0.0",
    "eslint-plugin-prettier": "^3.3.1",
    "mocha": "^9.1.3",
    "rimraf": "^3.0.2",
    "sinon": "^9.2.4",
    "supertest": "^6.1.6",
    "ts-node": "^10.8.1",
    "typescript": "^4.4.4"
  },
  "dependencies": {
    "@types/mocha": "^9.0.0",
    "@types/sinon": "^9.0.11",
    "aws-sdk": "^2.1150.0",
    "bcrypt": "^5.0.1",
    "body-parser": "^1.20.0",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "express-jwt": "^6.1.0",
    "express-validator": "^6.14.1",
    "helmet": "^4.6.0",
    "ioredis": "^4.28.0",
    "jsonwebtoken": "^8.5.1",
    "newrelic": "^8.14.0",
    "pg": "^8.7.3",
    "rollbar": "^2.25.0",
    "swagger-ui-express": "^4.4.0",
    "throng": "^5.0.0",
    "tsoa": "^4.1.0",
    "typeorm": "^0.2.39",
    "uuid": "^8.3.2"
  }
}
