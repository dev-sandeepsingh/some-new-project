{
  "name": "node_boilerplate_typescript",
  "version": "1.0.0",
  "private": true,
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "npm run build && node build/index.js",
    "start:dev": "npx nodemon ./src/index.ts",
    "build": "rimraf ./build && tsc",
    "typeorm": "node ./node_modules/typeorm/cli.js",
    "typeorm:dev": "npx cross-env NODE_ENV=development ts-node ./node_modules/typeorm/cli.js",
    "typeorm:test": "npx cross-env NODE_ENV=test ts-node ./node_modules/typeorm/cli.js",
    "db:update": "npm run build && npm run typeorm migration:run",
    "db:update:dev": "npm run typeorm:dev migration:run",
    "db:update:test": "npm run typeorm:test migration:run",
    "db:generate:dev": "npm run typeorm:dev migration:generate -- -n",
    "test": "npx cross-env NODE_ENV=test mocha --exit --timeout 15000 -r ts-node/register --file ./test/setup.ts './test/**/*.test.ts'",
    "test:watch": "npx cross-env NODE_ENV=test mocha --timeout 15000 -r ts-node/register -w -R min --file ./test/setup.ts './test/**/*.test.ts'",
    "lint": "eslint . --ext .ts",
    "lint-fix": "eslint . --ext .ts --fix"
  },
  "keywords": [],
  "author": "Sandeep Singh",
  "engines": {
    "node": "16.x"
  },
  "license": "ISC",
  "devDependencies": {
    "cross-env": "^7.0.3",
    "ts-node": "^10.8.1"
  },
  "dependencies": {
    "aws-sdk": "^2.1150.0",
    "bcrypt": "^5.0.1",
    "body-parser": "^1.20.0",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "express-jwt": "^6.1.0",
    "express-validator": "^6.14.1",
    "helmet": "^4.6.0",
    "newrelic": "^8.14.0",
    "pg": "^8.7.3",
    "rollbar": "^2.25.0",
    "swagger-ui-express": "^4.4.0",
    "throng": "^5.0.0",
    "typeorm": "^0.2.39"
  }
}
